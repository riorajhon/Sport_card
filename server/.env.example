# MongoDB (local or Atlas)
MONGODB_URI=mongodb://127.0.0.1:27017/vinted
# Server port
PORT=3001
# Vinted site. Use EU for buyable shipping in Europe (es, fr, de, it, nl, pl). com/uk often "shipping not available" outside US/UK.
VINTED_DOMAIN=es
# Search query for sport / trading cards
VINTED_SEARCH=sport card

# eBay Browse API (price comparison). Create app at https://developer.ebay.com/my/keys
EBAY_CLIENT_ID=
EBAY_CLIENT_SECRET=
# eBay marketplace (buy links): EBAY_ES, EBAY_DE, EBAY_FR, EBAY_IT, EBAY_US, EBAY_GB...
EBAY_MARKETPLACE_ID=EBAY_ES
